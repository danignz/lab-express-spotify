<div>
  {{#each albumsList}}
    {{#if this.images.1.url}}
      <img src="{{this.images.1.url}}" alt="Image of {{this.name}}" />
    {{else}}
      <img src="images/notimage.jpg" alt="Not image avariable" />
    {{/if}}
    <div>
      <h1>{{this.name}}</h1>
      <button><a href="/tracks/{{this.id}}">View Tracks</a></button>
    </div>
  {{/each}}
</div>